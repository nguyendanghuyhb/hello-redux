{
  "extends": ["airbnb-base", "prettier", "eslint:recommended", "plugin:react/recommended"],
  "plugins": ["import", "prettier", "react"],
  "rules": {
    "no-console": [
      "error",
      {
        "allow": ["warn", "error", "info"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "react/prop-types": [0]
  },
  "globals": {
    "document": true,
    "foo": true,
    "window": true
  }
}